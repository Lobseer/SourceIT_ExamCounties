<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>ServletUI</servlet-name>
    <servlet-class>servlet.ServletUI</servlet-class>
    <init-param>
      <param-name>param1</param-name>
      <param-value>First param</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletUI</servlet-name>
    <url-pattern>/ServletUI/*</url-pattern>
  </servlet-mapping>


  <welcome-file-list>
    <welcome-file>MainForm.jsp</welcome-file>
  </welcome-file-list>

</web-app>
